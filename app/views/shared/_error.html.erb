<% if user.errors %>
<div class='container'>
  <div class="alert alert-success alert-dismissable">
    <ul>
        <% user.errors.full_messages.each do |message| %>
            <i class="fas fa-check"></i>
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
            <li><%= message %></li>
        <% end %>
    </ul>
  </div>
</div>
<% end %>